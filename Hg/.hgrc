[ui]
username = Oling Cat <olingcat@gmail.com>
verbose = True
merge = internal:merge
#设置使用 hg latest 命令来查看最近的 5 条 log
[alias]
latest = log --limit 5
glog = glog --limit 25

#设置扩展
[extensions]
#设置graph后，可以用hg glog来查看分支结构
hgext.graphlog =
#设置书签扩展
hgext.bookmarks =
#清理未跟踪文件
hgext.purge =
#色彩输出
hgext.color =
#FileMerge
#extdiff=

